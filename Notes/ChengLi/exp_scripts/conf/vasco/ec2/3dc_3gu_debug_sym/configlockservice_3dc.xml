
<!--
  This file is used to configure the lock service for both server side and client side
  All sub-attributes must be put between two lockService tags.
  By Cheng
-->

<lockService>
    <lockServerCluster lsNum="3">
    	<lockServer lsId="0" lsIP="ec2-52-59-252-140.eu-central-1.compute.amazonaws.com" lsPort="10000" dbPort="10001"/>
    	<lockServer lsId="1" lsIP="ec2-54-86-73-106.compute-1.amazonaws.com" lsPort="10002" dbPort="10003"/>
    	<lockServer lsId="2" lsIP="ec2-54-193-123-214.us-west-1.compute.amazonaws.com" lsPort="10004" dbPort="10005"/>
    </lockServerCluster>
    <lockClientGroup lcNum="3">
    	<lockClient lcId="0" lcIP="ec2-52-59-248-19.eu-central-1.compute.amazonaws.com" lcPort="10016"/>
    	<lockClient lcId="1" lcIP="ec2-54-86-231-150.compute-1.amazonaws.com" lcPort="10017"/>
    	<lockClient lcId="2" lcIP="ec2-54-215-133-89.us-west-1.compute.amazonaws.com" lcPort="10018"/>
    </lockClientGroup>
    
    <collectionOfConflicts numOfConflicts="4">
        <conflict type="symmetry" leftOperand="registeruser" rightOperand="registeruser"/>
        <conflict type="symmetry" leftOperand="storebuynow" rightOperand="storebuynow"/>
        <conflict type="symmetry" leftOperand="closeauction" rightOperand="placebid"/>
        <conflict type="symmetry" leftOperand="placebid" rightOperand="closeauction"/>
    </collectionOfConflicts>
</lockService>
